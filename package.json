{
  "name": "stickets",
  "version": "0.2.0",
  "description": "Chrome Extension that allows you to add a notes to any webpage you visit as well as share it with whoever you want",
  "main": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run cleanup && npm run codegen && rollup -c",
    "dev": "npm run cleanup && concurrently \"npm run codegen --watch\" \"npm run dev:rollup\"",
    "dev:rollup": "rollup -c -w",
    "cleanup": "rm -rf ./src/common/graphql/__generated__",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "npm run codegen --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/b-ff/stickets.git"
  },
  "author": "Slava Biryukov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/b-ff/stickets/issues"
  },
  "homepage": "https://github.com/b-ff/stickets#readme",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
