    "@graphql-codegen/typescript-react-apollo": "3.2.14",
    "@graphql-codegen/urql-introspection": "2.1.1",
    "@graphql-eslint/eslint-plugin": "^3.10.4",
    "@linaria/babel-preset": "3.0.0-beta.15",
    "@linaria/rollup": "3.0.0-beta.15",
    "@linaria/shaker": "3.0.0-beta.15",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-image": "^2.1.1",
